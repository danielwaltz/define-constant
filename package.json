{
  "name": "ts-define-constant",
  "version": "1.2.2",
  "description": "Type safe utility for defining static constants with automatic type narrowing and additional helper functions",
  "license": "MIT",
  "author": "Daniel Waltz",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielwaltz/ts-define-constant.git"
  },
  "keywords": [
    "utility",
    "typescript",
    "constant",
    "narrowing",
    "type-narrowing"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint . --no-fix --ignore-path .gitignore --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:type-check": "vitest --typecheck.only",
    "test": "run-p test:unit test:type-check",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/node": "~20.10.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "semantic-release": "^22.0.12",
    "standard-version": "^9.5.0",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.3"
  }
}
