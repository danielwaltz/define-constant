{
  "name": "ts-define-constant",
  "version": "1.2.2",
  "description": "Type safe utility for defining static constants with automatic type narrowing and additional helper functions",
  "license": "MIT",
  "author": "Daniel Waltz",
  "repository": {
    "type": "git",
    "url": "https://github.com/danielwaltz/ts-define-constant.git"
  },
  "keywords": [
    "utility",
    "typescript",
    "constant",
    "narrowing",
    "type-narrowing"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --no-fix --ignore-path .gitignore --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:type-check": "vitest typecheck",
    "test": "run-p test:unit test:type-check",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.4",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/node": "~20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.8",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.1",
    "type-fest": "^4.8.2",
    "typescript": "~5.3.2",
    "vitest": "^0.34.6"
  }
}
