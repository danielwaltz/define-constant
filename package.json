{
  "name": "ts-define-constant",
  "version": "1.2.5",
  "description": "Type safe utility for defining static constants with automatic type narrowing and additional helper functions",
  "type": "module",
  "keywords": [
    "utility",
    "typescript",
    "constant",
    "narrowing",
    "type-narrowing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielwaltz/ts-define-constant.git"
  },
  "author": "Daniel Waltz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/.eslintcache --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:type-check": "vitest --typecheck.only",
    "test": "run-p test:unit test:type-check",
    "release": "standard-version",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@danielwaltz/eslint-config": "^0.0.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "~22.10.5",
    "eslint": "^9.18.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "standard-version": "^9.5.0",
    "type-fest": "^4.32.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "conventional-changelog-conventionalcommits": ">= 8.0.0"
  }
}
